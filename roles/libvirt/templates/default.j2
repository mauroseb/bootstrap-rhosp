<network>
  <name>default</name>
  <forward dev="{{ default_net_dev }}" mode='route'>
	<interface dev='{{ default_net_dev }}' />
  </forward>
  <bridge name="{{ default_bridge }}" stp='off' delay='0' />
  <mac address='52:54:00:c7:15:90' />
  <ip address='{{ default_net_ip }}' netmask='{{ default_net_netmask }}'/>
</network>
